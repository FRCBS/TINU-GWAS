#!/bin/bash

module load plink/1.90b4.1

cd /proj/2000066/IC3/TINU/IC1-IC3-FIN-combined

plink --bfile /proj/2000066/IC1/imputation/IC1_imputed_filtered0.5_data/modified_IC1_chr1_22_all.impute2_filtered0.5 --extract common.imputed.snps --make-bed --out IC1_imp_commonSNPs 

plink --file /proj/2000066/IC3/Imputed_and_filtered_data/modified_IC3F_chr1_22_all.impute2_filtered0.5 --extract common.imputed.snps --make-bed --out IC3_imp_commonSNPs 

plink --bfile IC1_imp_commonSNPs --bmerge IC3_imp_commonSNPs --make-bed --out IC1-IC3_imputed.merged


